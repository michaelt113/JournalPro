<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="sdp.journalpro">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".JE_Login_Home"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".JE_Base_Activity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".JE_Main_Home"
            android:label="@string/title_activity_main3"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".JE_New_Home"
            android:screenOrientation="portrait" />
        <activity
            android:name=".JE_Calendar_Activity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".JE_Entry_Home"
            android:label="@string/title_activity_main"
            android:screenOrientation="portrait"/>
        <activity android:name=".JE_Entry_History"/>
    </application>

</manifest>